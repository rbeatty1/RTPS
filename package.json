{
  "name": "rtsp",
  "version": "0.0.30",
  "description": "Tool to identify different types of transit gaps to assist in making data-driven decisions to evaluate and prioritize transit improvements throughout the Philadelphia region.",
  "main": "app.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js",
    "build": "webpack -p --watch",
    "test": "npm test",
    "licenses": "license-checker --json --relativeLicensePath --out dependency_licenses.json",
    "preversion": "npm run licenses",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Robert Beatty <rbeatty@dvrpc.org>",
  "license": "CC0-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dvrpc/RTPS.git"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/plugin-transform-template-literals": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.6",
    "babel-preset-minify": "^0.5.0",
    "core-js": "^3.2.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "lint-staged": "^7.0.0",
    "live-server": "^1.2.0",
    "prettier": "^1.10.2",
    "scrollmagic": "^2.0.6",
    "style-loader": "^0.21.0",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.7.2"
  },
  "optionalDependencies": {
    "license-checker": "^8.0.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "eslint": "^4.18.1",
    "file-loader": "^2.0.0",
    "mapbox-gl": "^0.48.0",
    "promise-polyfill": "^8.1.3",
    "whatwg-fetch": "^3.0.0"
  }
