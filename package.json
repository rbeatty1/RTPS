{
  "name": "rtsp",
  "version": "0.0.30",
  "description": "Tool to identify different types of transit gaps to assist in making data-driven decisions to evaluate and prioritize transit improvements throughout the Philadelphia region.",
  "main": "app.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js",
    "build": "webpack -p --watch",
    "test": "npm test",
    "licenses": "license-checker --json --relativeLicensePath --out dependency_licenses.json",
    "preversion": "npm run licenses",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Robert Beatty <rbeatty@dvrpc.org>",
  "license": "CC0-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dvrpc/RTPS.git"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "lint-staged": "^7.0.0",
    "live-server": "^1.2.0",
    "prettier": "^1.10.2",
    "scrollmagic": "^2.0.6",
    "style-loader": "^0.21.0",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^3.12.0",
    "webpack-cli": "^2.1.5",
    "webpack-dev-server": "^2.11.2"
  },
  "optionalDependencies": {
    "license-checker": "^8.0.4"
  },
  "dependencies": {
    "eslint": "^4.18.1",
    "file-loader": "^2.0.0",
    "flexbox": "0.0.3",
    "mapbox-gl": "^0.48.0"
  }
}
